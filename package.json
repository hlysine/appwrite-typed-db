{
  "name": "appwrite-typed-db",
  "version": "0.3.1",
  "main": "./dist/appwrite-typed-db.umd.js",
  "module": "./dist/appwrite-typed-db.es.js",
  "types": "./dist/appwrite-typed-db.d.ts",
  "type": "module",
  "license": "MIT",
  "description": "Strongly typed API for Appwrite Tables DB.",
  "homepage": "https://github.com/hlysine/appwrite-typed-db/",
  "repository": {
    "type": "git",
    "url": "https://github.com/hlysine/appwrite-typed-db.git"
  },
  "keywords": [
    "appwrite",
    "typescript",
    "database",
    "tables",
    "typed",
    "sdk",
    "tablesdb"
  ],
  "dependencies": {},
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.11",
    "@types/bun": "^1.2.21",
    "unplugin-dts": "^1.0.0-beta.6",
    "vite": "^7.1.5"
  },
  "peerDependencies": {
    "appwrite": "^20.0.0",
    "node-appwrite": "^19.0.0",
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "appwrite": {
      "optional": true
    },
    "node-appwrite": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/appwrite.d.ts",
      "import": "./dist/appwrite.es.js",
      "require": "./dist/appwrite.umd.js"
    },
    "./node": {
      "types": "./dist/node-appwrite.d.ts",
      "import": "./dist/node-appwrite.es.js",
      "require": "./dist/node-appwrite.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "bun test",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "bun run build"
  }
}
